                                        {
                                            "key": "individualName",
                                            "title": "Individual name",
                                            "type": "string"
                                        },
                                        {
                                            "key": "organisationName",
                                            "title": "Organisation name",
                                            "type": "string"
                                        },
                                        {
                                            "key": "positionName",
                                            "title": "Position",
                                            "type": "string"
                                        },
                                        {
                                            "key": "contactInfo.phone.voice",
                                            "title": "Voice",
                                            "type": "string"
                                        },
                                        {
                                            "key": "contactInfo.phone.facsimile",
                                            "title": "Facsimile",
                                            "type": "string"
                                        },
                                        {
                                            "key": "contactInfo.address.deliveryPoint",
                                            "title": "Delivery point",
                                            "type": "string"
                                        },
                                        {
                                            "key": "contactInfo.address.city",
                                            "title": "City",
                                            "type": "string"
                                        },
                                        {
                                            "key": "contactInfo.address.postalCode",
                                            "title": "Postal code",
                                            "type": "string"
                                        },
                                        {
                                            "key": "contactInfo.address.country",
                                            "title": "Country",
                                            "type": "string"
                                        },
                                        {
                                            "key": "contactInfo.address.elctronicMailAddress",
                                            "title": "Email",
                                            "type": "string"
                                        }
                                    ]
                                }
                            ]
                        }                                        
                    ]
                },
                {
                    "type": "section_container",
                    "key": "spatialRepresentationInfo_container",
                    "title": "Spatial representation information",
                    "items": [
                        {
                            "type": "section",
                            "key": "spatialRepresentationInfo_section",
                            "title": "Spatial representation information",
                            "items": [
                                {
                                    "key": "description.spatialRepresentationInfo",
                                    "type": "nested_array",
                                    "title": "Spatial representation information",
                                    "props": [
                                        {
                                            "key": "vectorSpatialRepresentation.topologyLevel",
                                            "title": "Topology level",
                                            "type": "dropdown",
                                            "enum":["geometryOnly", "topology1D", "planarGraph", "fullPlanarGraph", "surfaceGraph", "fullSurfaceGraph", "topology3D", "fullTopology3D", "abstract"]
                                        },
                                        {
                                            "key": "vectorSpatialRepresentation.geometricObjects",
                                            "title": "Geometric object definition",
                                            "type": "array",
                                            "props": [
                                                {
                                                    "key": "geometricObjectType",
                                                    "title": "Geometric Object type",
                                                    "type": "text"
                                                },
                                                {
                                                    "key": "geometricObjectCount",
                                                    "title": "Geometric Object count",
                                                    "type": "text"
                                                }
                                            ]
                                        },
                                        {
                                            "key": "gridSpatialRepresentation",
                                            "title": "gridSpatialRepresentation",
                                            "type": "nested_array",
                                            "props": [
                                                {
                                                    "key": "numberOfDimensions",
                                                    "title": "numberOfDimensions",
                                                    "type": "text"
                                                },
                                                {
                                                    "key":"axisDimensionProperties.dimension",
                                                    "title":"Axis dimensions",
                                                    "type":"array",
                                                    "props": [
                                                        {
                                                            "key": "dimensionName",
                                                            "title": "Dimension name",
